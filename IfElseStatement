package com.java;

public class ifelseStatement 
{
	public static void main(String[] args)
	{
		int a= 10,b = 20,c = 50;
		if(a>b && a>c)
		{ 
			System.out.println(a+" Is Greatest");
		}
		else if(b>a && b>c)
		{
			System.out.println(b+" Is Greatest");
		}
		else if(c>a && c>b)
		{
			System.out.println(c+" Is Greatest");
		}
	}
}
